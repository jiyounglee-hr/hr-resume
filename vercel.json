{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/generate_questions", "dest": "/api/generate_questions.py" },
    { "src": "/api/analyze", "dest": "/api/analyze.py" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.+)", "dest": "/$1" }
  ]
} 