{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/generate_questions/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/analyze/index.py"
    },
    {
      "src": "/api/generate_questions",
      "dest": "/api/generate_questions/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 